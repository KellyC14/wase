<?xml version="1.0" encoding="ISO-8859-1"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://wase.princeton.edu/" 
           xmlns="http://wase.princeton.edu/"
           elementFormDefault="qualified">
 

<!-- Type Definitions -->
	<!-- Simple -->
			 
	<xs:simpleType name="weekdaytypes">
		<xs:restriction base="xs:string">
			<xs:enumeration value="sunday"/>
			<xs:enumeration value="monday"/>
			<xs:enumeration value="tuesday"/>
			<xs:enumeration value="wednesday"/>
			<xs:enumeration value="thursday"/>
			<xs:enumeration value="friday"/>
			<xs:enumeration value="saturday"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="everytypes">
		<xs:restriction base="xs:string">
			<xs:enumeration value="daily"/>
			<xs:enumeration value="dailyweekdays"/>
			<xs:enumeration value="weekly"/>
			<xs:enumeration value="otherweekly"/>
			<xs:enumeration value="monthlyday"/>
			<xs:enumeration value="monthlyweekday"/>
		</xs:restriction>
	</xs:simpleType>
				
	<xs:simpleType name="accesstypes">
		<xs:restriction base="xs:string">
			<xs:enumeration value="open"/>
			<xs:enumeration value="limited"/>
			<xs:enumeration value="restricted"/>
			<xs:enumeration value="private"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="managerstatusvalues">
		<xs:restriction base="xs:string">
			<xs:enumeration value="active"/>
			<xs:enumeration value="pending"/>
			<xs:enumeration value="any"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="whichblocks">
		<xs:restriction base="xs:string">
			<xs:enumeration value="instance"/>
			<xs:enumeration value="series"/>
			<xs:enumeration value="seriesfromtoday"/>
            <xs:enumeration value="period"/>
            <xs:enumeration value="periodfromtoday"/>
		</xs:restriction>
	</xs:simpleType>						

	
	<xs:simpleType name="whichcals">
		<xs:restriction base="xs:string">
			<xs:enumeration value="all"/>
			<xs:enumeration value="fromtoday"/>
		</xs:restriction>
	</xs:simpleType>						


	<xs:simpleType name="blockdividevalues">
		<xs:restriction base="xs:string">
			<xs:enumeration value="multipleslots"/>
			<xs:enumeration value="singleslots"/>
		</xs:restriction>
	</xs:simpleType>						
	
	<xs:simpleType name="datestatusvalues">
		<xs:restriction base="xs:string">
			<xs:enumeration value="noblocks"/>
			<xs:enumeration value="blocksnoappsnoslots"/>
			<xs:enumeration value="blocksnoappsslots"/>
			<xs:enumeration value="blocksappsnoslots"/>
			<xs:enumeration value="blocksappsslots"/>
			<xs:enumeration value="blocksmyappsslots"/>
			<xs:enumeration value="blocksmyappsnoslots"/>
			<xs:enumeration value="myblocksnoappsnoslots"/>
			<xs:enumeration value="myblocksnoappsslots"/>
			<xs:enumeration value="myblocksappsnoslots"/>
			<xs:enumeration value="myblocksappsslots"/> 
			<xs:enumeration value="myblocksmyappsslots"/>
			<xs:enumeration value="myblocksmyappsnoslots"/>
		</xs:restriction>
	</xs:simpleType>						

				
	<!-- Complex -->
	
	<xs:complexType name="preftype">
		<xs:sequence>
			<xs:element name="keytag" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="val" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="class" type="xs:string" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="vartype">
		<xs:sequence>
			<xs:element name="var" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="val" type="xs:string" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
		
	<xs:complexType name="userinfo">
		<xs:sequence>
			<xs:element name="userid" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="name" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="phone" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="email" type="xs:string" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	
		
	<xs:complexType name="labelinfo">
		<xs:sequence>
			<xs:element name="NAMETHING" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="NAMETHINGS" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="APPTHING" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="APPTHINGS" type="xs:string" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>	

	
	 <xs:complexType name="managermemberinfo">
		 <xs:sequence>
			 <xs:element name="user" type="userinfo" minOccurs="0"/>
			 <xs:element name="calendarid" type="xs:decimal" minOccurs="0"/>
			 <xs:element name="status" type="managerstatusvalues"/>
			 <xs:element name="notify" type="xs:boolean" minOccurs="0"/>
			 <xs:element name="remind" type="xs:boolean" minOccurs="0"/>
			 <xs:element name="memberfutureapps" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
		 </xs:sequence>
	 </xs:complexType>
	 
	 <xs:complexType name="notifyandremind">
		<xs:sequence>
			<xs:element name="notify" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="notifyman" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="notifymem" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="remind" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="remindman" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="remindmem" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="apptmsg" type="xs:string" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="accessrestriction">
		<xs:sequence>		
			<xs:element name="viewaccess" type="accesstypes" minOccurs="0" maxOccurs="1"/>
			<xs:element name="viewulist" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="viewclist" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="viewglist" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="makeaccess" type="accesstypes" minOccurs="0" maxOccurs="1"/>
			<xs:element name="makeulist" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="makeclist" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="makeglist" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="showappinfo" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
		
	<xs:complexType name="label">
		<xs:sequence>		
			<xs:element name="NAMETHING" type="xs:string" minOccurs="0" maxOccurs="1"/>	
			<xs:element name="NAMETHINGS" type="xs:string" minOccurs="0" maxOccurs="1"/>		
			<xs:element name="APPTHING" type="xs:string" minOccurs="0" maxOccurs="1"/>		
			<xs:element name="APPTHINGS" type="xs:string" minOccurs="0" maxOccurs="1"/>			
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="uservalidation">
		<xs:sequence>
			<xs:element name="userid" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="password" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="userinfo" type="userinfo" minOccurs="0" maxOccurs="1"/>
			<xs:element name="isvalid" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="coursevalidation">
		<xs:sequence>
			<xs:element name="courseid" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="isvalid" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>	
	
	<xs:complexType name="groupvalidation">
		<xs:sequence>
			<xs:element name="groupid" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="isvalid" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	
		
	<xs:complexType name="statusvalidation">
		<xs:sequence>
			<xs:element name="statusid" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="isvalid" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="waiter">
		<xs:sequence>
			<xs:element name="waitid" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
			<xs:element name="calendarid" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
			<xs:element name="userid" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="name" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="email" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="textemail" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="phone" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="msg" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="startdatetime" type="xs:dateTime" minOccurs="1" maxOccurs="1"/>
			<xs:element name="enddatetime" type="xs:dateTime" minOccurs="1" maxOccurs="1"/>
			<xs:element name="whenadded" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	
	<xs:complexType name="courseinfo">
		<xs:sequence>
			<xs:element name="id" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="title" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="instructors" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="instructor" type="instructorinfo" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>	
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
	
	<xs:complexType name="instructorinfo">
		<xs:sequence>
			<xs:element name="userid" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="name" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="calendars" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="calendar" type="calendarheaderinfo" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>	
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	
	<xs:complexType name="calendarheaderinfo">
		<xs:sequence>
			<xs:element name="calendarid" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
			<xs:element name="title" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="owner" type="userinfo" minOccurs="0" maxOccurs="1"/>
			<xs:element name="waitlist" type="xs:boolean" minOccurs="0"/>
			<xs:element name="waitcount" type="xs:decimal" minOccurs="0"/>
			<xs:element name="labels" type="label" minOccurs="0"/>
			<xs:element name="ownerfutureapps" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="calendarinfo">
		<xs:complexContent>
			<xs:extension base="calendarheaderinfo">
				<xs:sequence>
					<xs:element name="location" type="xs:string" minOccurs="0"/>
					<xs:element name="notifyandremind" type="notifyandremind" minOccurs="0" maxOccurs="1"/>	
					<xs:element name="accessrestrictions" type="accessrestriction" minOccurs="0" maxOccurs="1"/>
					<xs:element name="purreq" type="xs:boolean" minOccurs="0"/>
					<xs:element name="available" type="xs:string" minOccurs="0"/>
					<xs:element name="overlapok" type="xs:boolean" minOccurs="0"/>
					<xs:element name="manager" type="managermemberinfo" minOccurs="0" maxOccurs="unbounded"/>  
					<xs:element name="member" type="managermemberinfo" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="icalpass" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	
	<xs:complexType name="calendarheaderinfowithmgrmem">
		<xs:complexContent>
			<xs:extension base="calendarheaderinfo">
				<xs:sequence>
					<xs:element name="manager" type="managermemberinfo" minOccurs="0" maxOccurs="unbounded"/>  
					<xs:element name="member" type="managermemberinfo" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<xs:complexType name="shortcalendarinfo">
		<xs:complexContent>
			<xs:extension base="calendarheaderinfo">
				<xs:sequence>
					<xs:element name="location" type="xs:string" minOccurs="0"/>
					<xs:element name="notifyandremind" type="notifyandremind" minOccurs="0" maxOccurs="1"/>	
					<xs:element name="accessrestrictions" type="accessrestriction" minOccurs="0" maxOccurs="1"/>
					<xs:element name="available" type="xs:string" minOccurs="0"/>
					<xs:element name="overlapok" type="xs:boolean" minOccurs="0"/>
					<xs:element name="purreq" type="xs:boolean" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<xs:complexType name="blockheaderinfo">
		<xs:sequence>
			<xs:element name="blockid" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
			<xs:element name="seriesid" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
			<xs:element name="calendarid" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
			<xs:element name="title" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="blockowner" type="userinfo" minOccurs="1" maxOccurs="1"/>
			<xs:element name="startdatetime" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="enddatetime" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="location" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="maxapps" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
			<xs:element name="maxper" type="xs:decimal" minOccurs="0" maxOccurs="1"/>	
			<xs:element name="opening" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
			<xs:element name="deadline" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
			<xs:element name="candeadline" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
			<xs:element name="available" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="mqkeaccess" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="labels" type="label" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType> 
	
	<xs:complexType name="blockheaderinfomakeable">
		<xs:complexContent> 
		<xs:extension base="blockheaderinfo">
			<xs:sequence>
				<xs:element name="makeable" type="xs:boolean" default="false" minOccurs="1" maxOccurs="1"/>
				<xs:element name="showappinfo" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
			</xs:sequence>
		</xs:extension>
	</xs:complexContent>
	</xs:complexType>					

	<xs:complexType name="blockinfo">
		<xs:complexContent>
			<xs:extension base="blockheaderinfo">
				<xs:sequence>
					<xs:element name="divideinto" type="blockdividevalues" minOccurs="0" maxOccurs="1"/>
					<xs:element name="slotsize" type="xs:decimal" minOccurs="0"/>
					<xs:element name="period" type="periodinfo" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="series" type="seriesinfo" minOccurs="0" maxOccurs="1"/>
					<xs:element name="notifyandremind" type="notifyandremind" minOccurs="0" maxOccurs="1"/>	
					<xs:element name="accessrestrictions" type="accessrestriction" minOccurs="0" maxOccurs="1"/>			
					<xs:element name="purreq" type="xs:boolean" minOccurs="0"/>
					<xs:element name="uid" type="xs:string" default=""/>
					<xs:element name="gid" type="xs:string" default=""/>
					<xs:element name="eid" type="xs:string" default=""/>
					<xs:element name="sequence" type="xs:decimal"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="periodinfo">	
		<xs:sequence> 
			<xs:element name="periodid" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
			<xs:element name="starttime" type="xs:time" minOccurs="1" maxOccurs="1"/>
			<xs:element name="duration" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
			<xs:element name="dayofweek" type="weekdaytypes" minOccurs="0" maxOccurs="1"/>
			<xs:element name="dayofmonth" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
			<xs:element name="weekofmonth" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="seriesinfo">	
		<xs:sequence> 
			<xs:element name="seriesid" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
			<xs:element name="startdate" type="xs:date" minOccurs="1" maxOccurs="1"/>
			<xs:element name="enddate" type="xs:date" minOccurs="1" maxOccurs="1"/>
			<xs:element name="every" type="everytypes" minOccurs="0" maxOccurs="1"/>
			<xs:element name="daytypes" type="xs:string" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	

	<xs:complexType name="slotinfo">
		<xs:sequence>
			<xs:element name="blockid" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
			<xs:element name="startdatetime" type="xs:dateTime" minOccurs="1" maxOccurs="1"/>
			<xs:element name="enddatetime" type="xs:dateTime" minOccurs="1" maxOccurs="1"/>
			<xs:element name="appt" type="apptheaderinfo" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>


	<!-- slotinfo with the number of appointments for each slot -->
	<xs:complexType name="fullslotinfo">
		<xs:complexContent>
			<xs:extension base="slotinfo">
				<xs:sequence>
					<xs:element name="numappts" type="xs:decimal" default="0" minOccurs="0" maxOccurs="1"/>
					<xs:element name="numapptslots" type="xs:decimal" default="0" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<!-- slotinfo with the number of appointments for each slot -->
	<xs:complexType name="fullslotinfomakeable">
		<xs:complexContent> 
			<xs:extension base="fullslotinfo">
				<xs:sequence>
					<xs:element name="makeable" type="xs:boolean" default="false" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	
	<!-- minOccurs for appointmentid is 0 for adding calendars -->
	<xs:complexType name="apptheaderinfo">
		<xs:sequence>
			<xs:element name="appointmentid" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
			<xs:element name="startdatetime" type="xs:dateTime" minOccurs="1" maxOccurs="1"/>
			<xs:element name="enddatetime" type="xs:dateTime" minOccurs="1" maxOccurs="1"/>
			<xs:element name="apptmaker" type="userinfo" minOccurs="0" maxOccurs="1"/>
			<xs:element name="blockid" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
			<xs:element name="calendarid" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
			<xs:element name="available" type="xs:string" minOccurs="0" maxOccurs="1"/>	
			<xs:element name="candeadline" type="xs:string" minOccurs="0" maxOccurs="1"/>		
		</xs:sequence>
	</xs:complexType>
	
	<!-- minOccurs for appointmentid is 0 for adding calendars -->
	<xs:complexType name="apptinfo">
		<xs:complexContent>
			<xs:extension base="apptheaderinfo">
				<xs:sequence>
					<xs:element name="purpose" type="xs:string" default="" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="venue" type="xs:string" default="" minOccurs="0" maxOccurs="1"/>
					<xs:element name="remind" type="xs:boolean" default="true" minOccurs="0" maxOccurs="1"/>
					<xs:element name="textemail" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="whenmade" type="xs:string" default="" minOccurs="1" maxOccurs="1"/>
					<xs:element name="madeby" type="xs:string" minOccurs="1" maxOccurs="1"/>
					<xs:element name="madebyname" type="xs:string" minOccurs="1" maxOccurs="1"/> 
					<xs:element name="uid" type="xs:string" default="" minOccurs="0" maxOccurs="1"/><!-- for iCal --> 	
					<xs:element name="gid" type="xs:string" default="" minOccurs="0" maxOccurs="1"/><!-- for Google --> 
					<xs:element name="eid" type="xs:string" default="" minOccurs="0" maxOccurs="1"/><!-- for Exchange--> 										
					<xs:element name="sequence" type="xs:decimal" minOccurs="0" maxOccurs="1"/><!-- for iCal -->
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<xs:complexType name="appdata">
		<xs:sequence>
			<xs:element name="appointmentid" type="xs:decimal"/>
			<xs:element name="blockid" type="xs:decimal"/>
			<xs:element name="calendarid" type="xs:decimal"/>
			<xs:element name="for_name" type="xs:string"/>
			<xs:element name="for_email" type="xs:string"/>
			<xs:element name="for_phone" type="xs:string"/>
			<xs:element name="for_userid" type="xs:string"/>
			<xs:element name="with_name" type="xs:string"/>
			<xs:element name="with_email" type="xs:string"/>
			<xs:element name="with_phone" type="xs:string"/>
			<xs:element name="with_userid" type="xs:string"/>
			<xs:element name="location" type="xs:string" minOccurs="0"/>
			<xs:element name="startdatetime" type="xs:time"/>
			<xs:element name="enddatetime" type="xs:time"/>
			<xs:element name="purpose" type="xs:string" default=""/>
            <xs:element name="venue" type="xs:string" default=""/>
			<xs:element name="whenmade" type="xs:string" default=""/>
		</xs:sequence>
	</xs:complexType>	
	
		
	<xs:complexType name="calendardateinfo">
		<xs:sequence>
